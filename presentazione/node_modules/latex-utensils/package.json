    "generate": "run-p 'generate:*'",
    "generate:bibtex": "run-p generate:bibtex:*",
    "generate:bibtex:simple": "pegjs -o out/src/bibtex/bibtex_parser_simple.js src/bibtex/bibtex.pegjs",
